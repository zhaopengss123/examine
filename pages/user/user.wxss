page{
  background: #F0F0F0;
}

.head{
  width: 100%;
  padding: 50rpx;
  background: #fff;
  box-sizing: border-box;
    border-bottom: solid 0.5px #e4e4e4;
}
.head navigator{
  height: 140rpx;
  padding: 10rpx;
  border-radius: 10rpx;
}
.head navigator.active{
  background: #f6f6f6;
}
.head-image{
  float: left;
  width: 140rpx;
  height: 140rpx;
  padding: 5rpx;
  border: solid 1px rgb(236, 236, 236);
  border-radius: 50%;
  box-sizing: border-box;
  overflow: hidden;
}
.head-image image{
  width: 100%;
  border-radius: 50%;
}
.head-name{
  padding: 30rpx 0 0 160rpx;
  font-size: 32rpx;
  line-height: 32rpx;
}
.head-phone{
  padding: 20rpx 0 0 160rpx;
  color: #666;
  font-size: 28rpx;
}


.item{
  padding: 0 30rpx;
  margin-top: 20rpx;
  font-size: 32rpx;
  line-height: 105rpx;
  background: #fff;
  border-bottom: solid 0.5px #e4e4e4;
  border-top: solid 0.5px #e4e4e4;
}

.item.active{
  background: #f6f6f6;
  box-shadow: 1px 1px 2px #ccc;
}


.footer{
  width: 100%;
  text-align: center;
  position: fixed;
  bottom: 50rpx;
}
.footer-phone{
   color: rgb(150, 150, 150);
  font-size: 26rpx;
  line-height: 36rpx;

}
.footer-time{
  color: rgb(150, 150, 150);
  font-size: 23rpx;
  line-height: 23rpx;
  padding-top: 15rpx;
}
.header{
  margin: 20rpx 0;
  padding: 40rpx 0;
  overflow: hidden;
  background: #fff;
  border-bottom: solid 0.5px #e4e4e4;
  border-top: solid 0.5px #e4e4e4;
}
.header   view {
  float: left;
  width: 33.33333%;
  text-align: center;
  box-sizing: border-box;
}
.header   view:not(:last-child){
  border-right: solid 1px #f5f5f5;
}
.header   view   text:first-child{
  display: block;
  padding-bottom: 17rpx;
  color: #FF9700;
  font-size: 36rpx;
  line-height: 36rpx;
}
.header   view   text:last-child{
  display: block;
  color: #4D4D4D;
  font-size: 24rpx;
  line-height: 24rpx;
}