.foots{
  width: 100%; 
  overflow: hidden;
  padding: 80rpx 0;
  background:#f5f6f8; 
   padding-bottom:180rpx;
   border-top:solid 0.5px #e4e4e4;

}
.shoptel{
    background: url(https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/store/icon-right-o.png) no-repeat right center;
    background-size: 15.2rpx 27.2rpx;
    width: 80%;
    padding-right: 12%;
}


.showfx{
  width: 100%;
  height: 100%;
  position:fixed;
  top: 0;
  left: 0;
  z-index: 100;
  display: none;
}
.showfxmain{
  width: 100%;
  height: 100%;
  position: relative;
}
.showfxbg{
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
}
.showfxcontent{
  width: 600rpx;
  height: 400rpx;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -250rpx;
  margin-left: -300rpx;
  z-index: 10;
  border-radius: 10rpx;
}
.showfxtitle{
  width: 100%;
  height: 100rpx;
  font-size: 36rpx;
  text-align: center;
  line-height: 100rpx;
}
.showfxnr{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
}
.showfxbtn{
  width: 600rpx;
  height:100rpx;
  overflow: hidden;
  margin-top: 80rpx;
}

.fxbtn{
  width: 299rpx;
  height: 100rpx;
  float: left;
  background: none;
  outline: none;
  text-align: center;
  line-height: 100rpx;
  border-top: solid 1rpx #ccc;
  border-right: solid 1px #ccc;
}
.fxbtn:nth-child(2){
  border-right: none;
  width: 300rpx;
  position: relative;
}
.fxbtn:nth-child(2) button{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
  opacity: 0;
}