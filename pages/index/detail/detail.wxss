page{
  background: #f5f6f8;
}

/* ------------------- header ------------------- */
.header{
  background: #fff;
  margin-bottom: 20rpx;
}
.head-cover{
  background: #999;
  height: 520.3125rpx;
  overflow: hidden;

}
.head-cover image{
  width: 750rpx;
  height: 520.3125rpx;
  
}
.slide-image{
   width: 750rpx;
  height: 520.3125rpx;

}
.swiper,swiper{
   width: 750rpx;
  height: 520.3125rpx;
}
.swiper img{
   width: 750rpx;
  height: 520.3125rpx;
}

.head-box{
  padding: 0 30rpx;
  border-bottom:  solid 0.5px #e4e4e4;
}
.head-name{
  padding: 33rpx 0 25rpx;
  font-size: 36rpx;
  line-height: 36rpx;
  font-weight: bold;
}
.head-name view{
  float: right;

  font-size: 26rpx;
  font-weight:normal;
  color:rgb(163, 163, 163);
}
.head-name view.active{
  color: #fff;
  background: #FF839A;
}
.head-address{
  padding-bottom: 30rpx;
  color: rgb(163, 163, 163);
  font-size: 26rpx;
  overflow: hidden;
}
.head-card{
  width:132rpx;
  height:40rpx;
  color: #fff;
  font-size: 21rpx;
  line-height: 40rpx;
  text-align: center;
  background: #ff839a;
  border-radius: 4rpx;
  position: absolute;
  top: 26rpx;
  right: 15rpx;
  z-index: 100;
}


.head-address text{
  display: block;
  width: 560rpx;
  float: left;

}
.head-address view{


    float: right;
  height: 32rpx;
  padding: 0 8rpx;
  color: #FF839A;
  font-size: 24rpx;
  line-height: 32rpx;
  transition: all .2s;
  border-radius: 4rpx;
}



/* -------------------- 活动 -------------------- */
.activity{
  margin-bottom: 20rpx;
  background: #fff;
  padding: 0 30rpx;
}
.activity-title{
  padding: 40rpx 0 32rpx;
  color: #333;
  font-size: 32rpx;
  border-bottom: solid 1px rgb(232, 232, 232);
}
.activity-item{
  position: relative;
  padding: 10rpx 0;
}
.activity-content .activity-item:not(:first-child){
  border-top: dashed 1px rgb(232, 232, 232);
}
.activity-item navigator{
  padding: 10rpx;
  border-radius: 10rpx;
}
.activity-item navigator.active{
  background: #f6f6f6;
}
.item-name{
  padding-top: 10rpx;
  padding-bottom: 20rpx;

  font-size: 32rpx;
  line-height: 32rpx;
  color: #000;
}
.item-detail{
  width: 500rpx;
  padding-bottom: 10rpx;
  color: #999;
  font-size: 24rpx;
  line-height: 30rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.item-price{
  padding-right: 26rpx;
  font-size: 36rpx;
  line-height: 36rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 16rpx 26rpx;
  position: absolute;
  top: 48rpx;
  right: 10rpx;
  color:#ff9000;
  font-weight: bold;
}


/* -------------------- 详情 -------------------- */
.storeinfo{
 background: #fff;
  padding:0 30rpx;
 
  border-top:solid 0.5px #e4e4e4;
}
.storeinfo-content{
 

}
.storeinfo-title{
  padding: 40rpx 0 32rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  border-bottom:solid 0.5px #e4e4e4;
}
.info-item{
  color: #63686e;
  font-size: 26rpx;
  position: relative;
  padding: 16rpx 0;
}
.info-item:not(:last-child){
  border-bottom:solid 0.5px #e4e4e4;
}
.info-item.function{
  line-height: 60rpx;
}
.info-item.function .item-icon{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  position: relative;
  top: auto;
  left: auto;
}
.info-item.function .info-cell{
  height: 60rpx;
  display: inline-block;
  margin-right: 30rpx;
  padding-left: 10rpx;
}
.item-icon{
  width: 28rpx;
  height: 88rpx;
  line-height: 88rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.item-icon image{
  width: 100%;
  vertical-align: middle;
}
.info-cell{
  line-height: 60rpx;
  padding-left: 36rpx;
}

/* ------------------- footer ------------------- */
.footer{
  width: 100%;
  color: #fff;
  font-size: 34rpx;
  line-height: 100rpx;
  text-align: center;
  background: #FF839A;
  position: fixed;
  bottom: 0;
  z-index: 9;
  transition: all .25s;
}
.footer.active{
  background: #fba0b1;
}
.footer-phone{
   color: rgb(150, 150, 150);
  font-size: 26rpx;
  line-height: 36rpx;
  text-align: center;

}
.footer-time{
  color: rgb(150, 150, 150);
  font-size: 23rpx;
  line-height: 23rpx;
  padding-top: 15rpx;
   text-align: center;
}
.footer1{
  width: 100%;
  color: #fff;
  font-size: 34rpx;
  line-height: 100rpx;
  text-align: center;
  background: #FF839A;
  position: fixed;
  bottom: 0;
  z-index: 9;
  transition: all .25s;
}
.footer1 view:nth-child(1){
  background: #fff;
  width: 25%;
  float: left;
  color: #FF839A;
}
.footer1 view:nth-child(2){
  width: 75%;
  float: left;
}
