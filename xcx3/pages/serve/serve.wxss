page{
  height: 100%;
  overflow: hidden;
}
.container{
  height: 100%;
  background: #f0f0f0;
  position: relative;
}

.swiper-tab{
  display: flex;
  width: 100%;
  height: 82rpx;
  line-height: 82rpx;
  border-top: solid 1px #f0f0f0;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
}
.swiper-list{
  flex: 1;
  color: #a1a1a1;
  font-size: 26rpx;
  text-align: center;
  transition: all .25s;
}
.swiper-list.active{
  color: #333;
  font-size: 28rpx;
}



.swiper-box{
  padding-top: 82rpx;
  height: 100%;
  box-sizing: border-box;
}

.serve-list{
  height: auto;
  overflow-y: auto;
  -webkit-overflow-scrolling : touch;
}
.serve-list .serve-item:first-child{
  margin-top: 22rpx;
}
.serve-item{
  background: #fff;
  margin-bottom: 21rpx;
  padding: 20rpx 30rpx;
}
.item-title{
  padding: 20rpx 0 22rpx;
  color: #000;
  font-size: 32rpx;
  line-height: 32rpx;
  border-bottom: solid 1px #eee;
}
.item-status{
  float: right;
  color: #F38A03;
  font-size: 22rpx;
}
.item-detail{
  height: 200rpx;
  padding: 22rpx 0;
  border-bottom: solid 1px #eee;
}
.item-detail view{
  color: rgb(161,161,161);
  font-size: 26rpx;
  line-height: 48rpx;
  padding-left: 20rpx;
}
.item-detail .item-detail-title{
  color: #000;
  font-size: 28rpx;
  line-height: 28rpx;
  padding-top: 14rpx;
  padding-bottom: 10rpx;
}
.item-detail .item-detail-image{
  width: 200rpx;
  height: 200rpx;
  padding: 0;

  border-radius: 10rpx;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
}
.item-detail .item-detail-image image{
  width: 200rpx;
  height: 200rpx;
  padding-top: 50rpx;
}
.item-detail navigator{
  height: 200rpx;
  border-radius: 10rpx;
  position: relative;
}
.item-detail navigator.active{
  background: #eee;
}
.item-type{
  color: #000;
  font-size: 28rpx;
  line-height: 84rpx;
  border-bottom: solid 1px #eee;
  overflow: hidden;
}
.item-type > view{
  float: right;
}
.item-type > view text{
  margin-left: 15rpx;
}
.item-footer{
  overflow: hidden;
}
.item-head{
  float: left;
  margin-top: 17rpx;
}
.item-head image{
  width: 82rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.item-head text{
  margin-left: 40rpx;
  font-size: 32rpx;
  line-height: 82rpx;
}
.item-head + .item-btns{
  margin-top: 8rpx;
}
.item-btns{
  float: right;
  padding-top: 18rpx;
  height: 66rpx;
}
.item-btns > view{
  display: inline-block;
  vertical-align: top;
  padding: 0 35rpx;
  height: 66rpx;
  line-height: 64rpx;
  margin-left: 15rpx;
  box-sizing: border-box;
  border-style: solid;
  border-width: 1px;
  border-radius: 8rpx;
  transition: all .15s;
}
.item-btn{
  color: rgb(161,161,161);
  font-size: 26rpx;
  border-color: #fff;
}
.item-btn.active{
  color: #fff;
  background: rgb(161,161,161);
}
.item-btn-active{
  color: #F38A03;
  font-size: 28rpx;
  border-color: #F38A03;
}
.item-btn-active.active{
  color: #fff;
  background: #F38A03;
}
.wkong{
  text-align: center;
  font-size: 32rpx;
  padding: 30rpx;
  color: #666;
}
swiper-item{
  overflow: scroll;
}